                        <then>
                            <output expression="B &amp; &quot; &gt; &quot; &amp; A &amp; &quot; &gt; &quot; &amp; C" newline="True"/>
                        </then>
                        <else>
                            <if expression="B &gt; C">
                                <then>
                                    <output expression="B &amp; &quot; &gt; &quot; &amp; C &amp; &quot; &gt; &quot; &amp; A" newline="True"/>
                                </then>
                                <else>
                                    <output expression="C &amp; &quot; &gt; &quot; &amp; B &amp; &quot; &gt; &quot; &amp; A" newline="True"/>
                                </else>
                            </if>
