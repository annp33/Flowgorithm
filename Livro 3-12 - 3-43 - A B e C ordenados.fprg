    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="A, B, C" type="Real" array="False" size=""/>
            <output expression="&quot;Entre com o primeiro n&#250;mero: &quot;" newline="True"/>
            <input variable="A"/>
            <output expression="&quot;Entre com o segundo n&#250;mero: &quot;" newline="True"/>
            <input variable="B"/>
            <output expression="&quot;Entre com o terceiro n&#250;mero: &quot;" newline="True"/>
            <input variable="C"/>
            <if expression="A &gt; B">
                <then>
                    <if expression="B &gt; C">
                        <then>
                            <output expression="A &amp; &quot; &gt; &quot; &amp; B &amp; &quot; &gt; &quot; &amp; C" newline="True"/>
                        </then>
